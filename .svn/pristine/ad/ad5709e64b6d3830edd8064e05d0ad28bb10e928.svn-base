<Window x:Class="bien4read.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bien4read"
        mc:Ignorable="d"
        Title="bien4read" Height="360" Width="626" Icon="1438601654_menu-24.ico">
    <Grid>
        <Button x:Name="btnPath" Content="경로선택" HorizontalAlignment="Left" Margin="432,10,0,0" VerticalAlignment="Top" Width="176" Click="btnPath_Click"/>
        <TextBox x:Name="tbPath" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="경로를선택하세요" VerticalAlignment="Top" Width="417" IsReadOnly="True"
                 Drop="tbPath_Drop" AllowDrop="True" PreviewDragEnter="tbPath_PreviewDragEnter" PreviewDragOver="tbPath_PreviewDragOver"
                 />
        <Button x:Name="btnChange" Content="변환" HorizontalAlignment="Left" Margin="432,288,0,0" VerticalAlignment="Top" Width="186" Click="btnChange_Click"/>
        <TextBox x:Name="tbMsg" HorizontalAlignment="Left" Height="272" Margin="10,38,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="417" IsReadOnly="True" AcceptsReturn="True"
                 ScrollViewer.VerticalScrollBarVisibility="Auto"
                 ScrollViewer.CanContentScroll="True"
                 />
        <Label x:Name="label" Content="빈줄삽입행수&#xD;&#xA;" HorizontalAlignment="Left" Margin="432,38,0,0" VerticalAlignment="Top" Height="30"/>
        <TextBox x:Name="tbAddEmptyLine" HorizontalAlignment="Left" Height="23" Margin="528,40,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="58"/>
        <Label x:Name="label_Copy" Content="줄" HorizontalAlignment="Left" Margin="586,38,0,0" VerticalAlignment="Top" Height="30"/>

    </Grid>
</Window>
