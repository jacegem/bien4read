<Window x:Class="bien4read.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bien4read"
        mc:Ignorable="d"
        Title="bien4read" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnPath" Content="경로선택" HorizontalAlignment="Left" Margin="432,10,0,0" VerticalAlignment="Top" Width="75" Click="btnPath_Click"/>
        <TextBox x:Name="tbPath" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="경로를선택하세요" VerticalAlignment="Top" Width="417" IsReadOnly="True"/>
        <Button x:Name="btnChange" Content="변환" HorizontalAlignment="Left" Margin="432,239,0,0" VerticalAlignment="Top" Width="75" Click="btnChange_Click"/>

    </Grid>
</Window>
